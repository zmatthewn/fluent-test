{
  "policies": [
    {
      "handler": {
        "export": "SetHeadersInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "headers": [
            {
              "name": "Authorization",
              "value": "$env(BOOMI_EVENT_STREAMS_TOKEN)"
            }
          ]
        }
      },
      "name": "boomi-event-streams-bearer-token-inbound",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/fluent-signature-verify-hmac-auth-inbound)"
      },
      "name": "fluent-signature-verify-hmac-auth-inbound",
      "policyType": "custom-code-inbound"
    },
    {
      "handler": {
        "export": "BasicAuthInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "accounts": [
            {
              "data": {
                "name": "John Doe",
                "email": "john.doe@gmail.com"
              },
              "password": "$env(CROWDTWIST_FLUENT_PASSWORD)",
              "username": "$env(CROWDTWIST_FLUENT_USERNAME)"
            }
          ],
          "allowUnauthenticatedRequests": false
        }
      },
      "name": "crowdtwist-fluent-basic-auth-inbound",
      "policyType": "basic-auth-inbound"
    }
  ]
}